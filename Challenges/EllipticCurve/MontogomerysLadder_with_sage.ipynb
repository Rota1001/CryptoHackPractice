    "from sage.all import *\n",
    "p = (1 << 255) - 19\n",
    "F = GF(p)\n",
    "# y^2 + a_1 xy + a_3 y = x^3 + a_2 x^2 + a_4 x + a_6\n",
    "A = 486662\n",
    "E = EllipticCurve(F, [0, A, 0, 1, 0])\n",
    "G = E.lift_x(F(9))\n",
    "n = 0x1337c0decafe\n",
    "print((n * G)[0])"
   ]
  }
